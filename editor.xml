<application>
  <component name="DaemonCodeAnalyzerSettings" profile="Kotlin All">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Monaco" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="LiveEditOptions" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true" useJBChromeExtension="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>